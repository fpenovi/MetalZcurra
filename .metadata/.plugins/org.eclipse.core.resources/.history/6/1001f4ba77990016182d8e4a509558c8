#include "Calculadora.h"

Calculadora::Calculadora() {
	this->numeroActual = 0;
}

Calculadora::~Calculadora() {
	// TODO Auto-generated destructor stub
}

int Calculadora::calcular(int hasta) {

	for (int i=0; i<hasta; i++) {
		this->numeroActual++;
	}

	return this->numeroActual;
}
